{
  "name": "@zuzjs/core",
  "version": "0.3.5",
  "keywords": [
    "core",
    "zuz",
    "zuz.js",
    "zuz orm",
    "zuzjs"
  ],
  "description": "ZuzJS Core Library",
  "author": "Zuz.js Team <support@zuz.com.pk>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:js": "tsup --watch",
    "dev": "npm-run-all -p dev:*",
    "build": "rm -rf dist && tsup",
    "publish": "npm run build && node publish-pre.js && npm publish && node publish-post.js"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "sideEffects": [
    "reflect-metadata"
  ],
  "dependencies": {
    "@types/hashids": "^2.0.0",
    "@types/md5": "^2.3.6",
    "axios": "^1.13.4",
    "date-fns": "^4.1.0",
    "hashids": "^2.3.0",
    "ip-regex": "^5.0.0",
    "js-cookie": "^3.0.5",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.2.3"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}